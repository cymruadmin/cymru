<!doctype html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>{{ renderData.title or title or metadata.title }}</title>
    <meta name="Description" content="{{ renderData.description or description or metadata.description }}">
    <link rel="stylesheet" href="{{ '/css/index.css' | url }}">
    <link rel="stylesheet" href="{{ '/css/prism-base16-monokai.dark.css' | url }}">
    <link rel="alternate" href="{{ metadata.feed.path | url }}" type="application/atom+xml" title="{{ metadata.title }}">
  </head>
  <body>
    <div class="eye-catch">
      <header class="header__flexcontainer">
        <img src="img/header.png">
        <nav class="header__flexitem">
          <ul class="header__nav--flexcontainer">
            <li class="header__nav--flexitem"><a class="header__nav--flexitem--link" href="#about">本学会について</a></li>
            <li class="header__nav--flexitem"><a class="header__nav--flexitem--link" href="#desc">概要</a></li>
            <li class="header__nav--flexitem"><a class="header__nav--flexitem--link" href="#docs">資料</a></li>
            <li class="header__nav--flexitem"><a class="header__nav--flexitem--link" href="mailto:nihoncymrugakkai@gmail.com">お問い合わせ</a></li>
          </ul>
        </nav>
      </header>
      <main{% if templateClass %} class="{{ templateClass }}"{% endif %}>
        {{ content | safe }}
      </main>
    </div>

    <footer></footer>

    <!-- Current page: {{ page.url | url }} -->
  </body>
</html>
